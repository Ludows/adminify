{
    "name": "ludows/adminify",
    "description": "A little admin package extremely extendable",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "artesaos/seotools": "^1.1.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.5.1",
        "kris/laravel-form-builder": "^1.52.0",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^10.8.0",
        "laravel/ui": "^3.1",
        "laravel/tinker": "^2.8.1",
        "spatie/laravel-backup": "^8.1.8",
        "spatie/laravel-menu": "^4.1.2",
        "spatie/laravel-permission": "^5.1",
        "spatie/laravel-searchable": "^1.11",
        "spatie/laravel-translatable": "^6.5",
        "spatie/menu": "^3.2.0",
        "thunderer/shortcode": "^0.7",
        "ycs77/laravel-form-builder-bs4": "^1.0.2",
        "barryvdh/laravel-debugbar": "^3.7.0",
        "spatie/laravel-feed" : "^4.2.1",
        "bpocallaghan/generators": "^9.0",
        "league/mime-type-detection" : "^1.11.0",
        "haydenpierce/class-finder" : "^0.5.1",
        "league/glide-laravel": "^1.0.0",
        "snowfire/beautymail": "dev-master",
        "cswni/laravel-env-set-command" : "^1.4",
        "anhskohbo/no-captcha": "^3.5",
        "spatie/laravel-sitemap" : "^6.2.5",
        "inertiajs/inertia-laravel" :"^0.6.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "facade/ignition": "^2.17.7",
        "laracasts/generators": "^2.0.2",
        "phpunit/phpunit": "^10.0.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ludows\\Adminify\\AdminifyServiceProvider"            
            ],
            "aliases": {
                "Adminify": "Ludows\\Adminify\\Facades\\Adminify"
            }
        }
    },
    "autoload": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "classmap": [
            "database/seeders"   
        ],
        "psr-4": {
            "Ludows\\Adminify\\" : "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ludows\\Adminify\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan adminify:env",
            "@php artisan adminify:install"
        ]
    }
}
